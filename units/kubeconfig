apiVersion: v1
      kind: Config
      users:
      - name: kubelet
        user:
          token: ${kubelet_token}
      clusters:
      - name: local
        cluster:
          insecure-skip-tls-verify: true
      contexts:
      - context:
          cluster: local
          user: kubelet
        name: service-account-context
      current-context: service-account-context
